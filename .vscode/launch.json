{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "prod",
      "request": "launch",
      "type": "dart",
      "cwd": "${workspaceFolder}",
      "args": [
        "--flavor",
        "prod",
        "--web-port",
        "3001",
        "--web-renderer",
        "canvaskit"
      ], // Remember to change the "program" property too
      "program": "lib/main_prod.dart"
    },
    {
      "name": "prod (attach)",
      "request": "attach",
      "type": "dart",
      "cwd": "${workspaceFolder}",
      "args": [
        "--flavor",
        "prod",
        "--web-port",
        "3001",
        "--web-renderer",
        "canvaskit"
      ], // Remember to change the "program" property too
      "program": "lib/main_prod.dart"
    },
    {
      "name": "prod (profile)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": [
        "--flavor",
        "prod",
        "--web-port",
        "3001",
        "--web-renderer",
        "canvaskit"
      ], // Remember to change the "program" property too
      "cwd": "${workspaceFolder}",
      "program": "lib/main_prod.dart"
    },
    {
      "name": "prod (release)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": [
        "--flavor",
        "prod",
        "--web-port",
        "3001",
        "--web-renderer",
        "canvaskit"
      ], // Remember to change the "program" property too
      "cwd": "${workspaceFolder}",
      "program": "lib/main_prod.dart"
    },
    /// Biblekids
    {
      "name": "old kids_prod (debug)",
      "request": "launch",
      "type": "dart",
      "cwd": "${workspaceFolder}",
      "args": [
        "--flavor",
        "kids_prod",
        "--web-port",
        "3001",
        "--web-renderer",
        "canvaskit"
      ], // Remember to change the "program" property too
      "program": "lib/main_kids_prod.dart"
    },
    /// Biblekids release
    {
      "name": "old kids_prod (release)",
      "request": "launch",
      "type": "dart",
      "cwd": "${workspaceFolder}",
      "flutterMode": "release",
      "args": [
        "--flavor",
        "kids_prod",
        "--web-port",
        "3001",
        "--web-renderer",
        "canvaskit"
      ], // Remember to change the "program" property too
      "program": "lib/main_kids_prod.dart"
    },
    {
      "name": "Debug build_runner",
      "cwd": "${workspaceFolder}/kids",
      "request": "launch",
      "program": ".dart_tool/build/entrypoint/build.dart",
      "type": "dart",
      "args": ["build", "--delete-conflicting-outputs", "--verbose"]
    },
    {
      "name": "Kids: prod",
      "request": "launch",
      "type": "dart",
      "cwd": "${workspaceFolder}/kids",
      "args": [
        "--flavor",
        "prod",
        "--web-port",
        "3001",
        "--web-renderer",
        "canvaskit"
      ], // Remember to change the "program" property too
      "program": "lib/main_prod.dart"
    },
    {
      "name": "Kids: Debug build_runner",
      "cwd": "${workspaceFolder}/kids",
      "request": "launch",
      "program": ".dart_tool/build/entrypoint/build.dart",
      "type": "dart",
      "args": ["build", "--delete-conflicting-outputs", "--verbose"]
    }
  ]
}
