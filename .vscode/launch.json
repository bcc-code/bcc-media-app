  "version": "0.2.0",
  "configurations": [
    {
      "name": "prod",
      "request": "launch",
      "type": "dart",
      "cwd": "${workspaceFolder}",
      "args": [
        "--flavor",
        "prod",
        "--web-port",
        "3001",
        "--web-renderer",
        "canvaskit"
      ], // Remember to change the "program" property too
      "program": "lib/main_prod.dart"
    },
    {
      "name": "prod (profile)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": [
        "--flavor",
        "prod",
        "--web-port",
        "3001",
        "--web-renderer",
        "canvaskit"
      ], // Remember to change the "program" property too
      "cwd": "${workspaceFolder}",
      "program": "lib/main_prod.dart"
    },
    {
      "name": "prod (release)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": [
        "--flavor",
        "prod",
        "--web-port",
        "3001",
        "--web-renderer",
        "canvaskit"
      ], // Remember to change the "program" property too
      "cwd": "${workspaceFolder}",
      "program": "lib/main_prod.dart"
    },
    /// Biblekids
    {
      "name": "kids_prod",
      "request": "launch",
      "type": "dart",
      "cwd": "${workspaceFolder}",
      "args": [
        "--flavor",
        "kids_prod",
        "--web-port",
        "3001",
        "--web-renderer",
        "canvaskit"
      ], // Remember to change the "program" property too
      "program": "lib/main_kids_prod.dart"
